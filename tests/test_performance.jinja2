<head>{{ title }}</head>
<body>
  <div>
    Hello down there, {{ name }}!
  </div>
  <table>
    <thead>
      <tr>
        {% for column_name in column_names %}
          <th>{{ column_name }}</th>
        {% endfor %}
      </tr>
    </thead>
    <tbody>
      {% for row in rows %}
        <tr>
          <td>{{ row.a }}</td>
          <td>{{ row.b }}</td>
        </tr>
      {% endfor %}
    </tbody>
  </table>
</body>
